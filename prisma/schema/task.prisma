model Alarm {
  uuid String @id @default(uuid())

  hour       Int
  minute     Int
  repeatDays Int[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Todo {
  uuid String @id @default(uuid())

  name       String
  targetDate DateTime

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
